@using static DevHunter.Common.NotificationMessagesConstants;

@if (TempData.ContainsKey(ErrorMessage))
{
	<script>customToastrError('@TempData[ErrorMessage]')</script>
}

@if (TempData.ContainsKey(WarningMessage))
{
	<script>customToastrWarning('@TempData[WarningMessage]')</script>
}

@if (TempData.ContainsKey(InformationMessage))
{
	<script>customToastrInfo('@TempData[InformationMessage]')</script>
}

@if (TempData.ContainsKey(SuccessMessage))
{
	<script>customToastrSuccess('@TempData[SuccessMessage]')</script>
}